<Window x:Class="Practice.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" >
    <Grid>
        <Canvas>
            <Rectangle Canvas.Left="20" Canvas.Top="20" Width="100" Height="100" Fill="Red" Panel.ZIndex="0"/>
            <Rectangle Canvas.Left="20" Canvas.Bottom="30" Width="100" Height="100" Fill="Blue" Panel.ZIndex="2"/>
            <Rectangle Canvas.Right="100" Canvas.Bottom="100" Width="100" Height="100" Fill="Yellow" Panel.ZIndex="1"/>
            <Button Canvas.Left="40" Canvas.Top="40" Content="AAA" FontSize="20"/>
        </Canvas>
    </Grid>
</Window>
