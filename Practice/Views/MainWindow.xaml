<Window x:Class="Practice.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" >
    <Grid>
        <Canvas>
            <Ellipse Canvas.Left="100" Canvas.Top="20" Width="50" Height="50" Fill="Red" Panel.ZIndex="1"/>
            <Ellipse Canvas.Left="50" Canvas.Top="100" Width="50" Height="50" Fill="Blue" Panel.ZIndex="1"/>
            <Ellipse Canvas.Left="150" Canvas.Top="100" Width="50" Height="50" Fill="Yellow" Panel.ZIndex="1"/>
            <Path Stroke="Black" StrokeThickness="1">
                <Path.Data>
                    <PathGeometry>
                        <PathGeometry.Figures>
                            <PathFigure StartPoint="125,65">
                                <LineSegment Point="75,100"/>
                            </PathFigure>
                            <PathFigure StartPoint="125,65">
                                <LineSegment Point="175,100"/>
                            </PathFigure>
                        </PathGeometry.Figures>
                    </PathGeometry>
                </Path.Data>
            </Path>
            <Button Canvas.Left="105" Canvas.Top="40" Content="押せ！！" FontSize="10" Click="Button_Click_1" Panel.ZIndex="2"/>
        </Canvas>
    </Grid>
</Window>
